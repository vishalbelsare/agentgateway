{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ExpressionContext",
  "type": "object",
  "properties": {
    "request": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "method": {
          "type": "string"
        },
        "uri": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "headers": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "body": {
          "type": [
            "array",
            "string",
            "null"
          ],
          "items": {
            "type": "integer",
            "format": "uint8",
            "minimum": 0,
            "maximum": 255
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "method",
        "uri",
        "path",
        "headers"
      ]
    },
    "response": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0,
          "maximum": 65535
        }
      },
      "additionalProperties": false,
      "required": [
        "code"
      ]
    },
    "jwt": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "inner": {
          "type": "object",
          "additionalProperties": true
        }
      },
      "required": [
        "inner"
      ]
    },
    "llm": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "streaming": {
          "type": "boolean"
        },
        "requestModel": {
          "type": "string"
        },
        "responseModel": {
          "type": [
            "string",
            "null"
          ]
        },
        "provider": {
          "type": "string"
        },
        "inputTokens": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "outputTokens": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "totalTokens": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0
        },
        "prompt": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "object",
            "properties": {
              "role": {
                "type": "string"
              },
              "content": {
                "type": "string"
              }
            },
            "required": [
              "role",
              "content"
            ]
          }
        },
        "completion": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "params": {
          "type": "object",
          "properties": {
            "temperature": {
              "type": [
                "number",
                "null"
              ],
              "format": "double"
            },
            "top_p": {
              "type": [
                "number",
                "null"
              ],
              "format": "double"
            },
            "frequency_penalty": {
              "type": [
                "number",
                "null"
              ],
              "format": "double"
            },
            "presence_penalty": {
              "type": [
                "number",
                "null"
              ],
              "format": "double"
            },
            "seed": {
              "type": [
                "integer",
                "null"
              ],
              "format": "int64"
            },
            "max_tokens": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0
            }
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "streaming",
        "requestModel",
        "provider",
        "params"
      ]
    },
    "source": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "address": {
          "type": "string",
          "format": "ip"
        },
        "port": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0,
          "maximum": 65535
        },
        "identity": {
          "type": [
            "object",
            "null"
          ],
          "properties": {
            "trustDomain": {
              "type": "string"
            },
            "namespace": {
              "type": "string"
            },
            "serviceAccount": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "required": [
            "trustDomain",
            "namespace",
            "serviceAccount"
          ]
        }
      },
      "additionalProperties": false,
      "required": [
        "address",
        "port"
      ]
    }
  },
  "additionalProperties": false
}