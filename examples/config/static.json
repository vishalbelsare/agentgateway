{
  "type": "static",
  "listener": {
    "type": "sse",
    "host": "0.0.0.0",
    "port": 8001,
    "authn": {
      "type": "jwt",
      "issuer": ["me"],
      "audience": ["me.com"],
      "jwks": {
        "type": "local",
        "source": {
          "type": "file",
          "data": "manifests/jwt/pub-key"
        }
      }
    }
  },
  "policies": [
    {
      "key": "sub",
      "value": "me",
      "resource": {
        "type": "tool",
        "id": "*"
      },
      "matcher": {
        "type": "equals"
      }
    }
  ],
  "targets": [
    {
      "name": "everything",
      "spec": {
        "type": "stdio",
        "cmd": "npx",
        "args": [
          "@modelcontextprotocol/server-everything"
        ]
      }
    }
  ]
}